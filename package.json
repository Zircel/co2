{
  "name": "co2",
  "version": "0.0.1",
  "description": "Later",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode production", 
    "dev": "concurrently --kill-others \"npm run build:dev\" \"npm run prettier-watch\" \"npm run prettier-watch-css\"",
    "build:dev": "cross-env NODE_ENV=development webpack-dev-server --mode development",
    "prettier-watch": "onchange '**/*.js' -- prettier --write {{changed}} --tab-width 3 --no-semi --single-quote",
    "prettier-watch-css": "onchange '**/*.css' -- prettier --write {{changed}} --tab-width 3 --single-quote"
  },
  "author": "Zircel",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.0",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "onchange": "^5.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.17.1",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  }
}
